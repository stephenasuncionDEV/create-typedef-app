{
  "name": "create-typedef-app",
  "description": "Tech stack template by Stephen Asuncion",
  "version": "0.0.1",
  "keywords": [
    "create-typedef-app"
  ],
  "author": "StephenAsuncion",
  "publisher": "StephenAsuncion",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenasuncionDEV/create-typedef-app.git"
  },
  "homepage": "https://github.com/stephenasuncionDEV/create-typedef-app#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/stephenasuncionDEV"
  },
  "bugs": {
    "url": "https://github.com/stephenasuncionDEV/create-typedef-app/issues"
  },
  "scripts": {
    "dev": "next dev",
    "dev:no-cache": "rm -rf .next/cache && yarn dev",
    "dev:prisma": "npx prisma generate",
    "lint": "next lint",
    "lint:types": "tsc --pretty --noEmit",
    "lint:format": "prettier --check .",
    "lint:staged": "yarn lint && yarn lint:types && yarn lint:format",
    "format": "prettier --write .",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true yarn build",
    "node:switch": "for /f usebackq %i in (`type .nvmrc`) do nvm use %i",
    "graphql:compile": "graphql-codegen -c src/codegen.ts",
    "graphql:watch": "graphql-codegen -w -c src/codegen.ts",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@apollo/server": "^4.2.2",
    "@as-integrations/next": "^1.2.0",
    "@chakra-ui/react": "^2.4.2",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^4.7.1",
    "@react-icons/all-files": "^4.1.0",
    "@vercel/og": "^0.0.21",
    "cookies-next": "^2.1.1",
    "crypto-js": "^4.1.1",
    "framer-motion": "^7",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "next": "13.0.6",
    "next-auth": "^4.18.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/client-preset": "^1.2.1",
    "@next/bundle-analyzer": "^13.0.6",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.0",
    "cross-env": "^7.0.3",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "prisma": "^4.7.1",
    "sass": "^1.56.1",
    "typescript": "4.9.3"
  }
}
